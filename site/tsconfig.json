{
	"compilerOptions": {
		"target": "es2020",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": false,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"paths": {
			"@/components/*": ["./src/components/*"],
			"@/styles/*": ["./src/styles/*"]
		}
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"]
}
